                    <TR>
                      <TD class=3Dcode>#include &lt;18f6722.h&gt; =
<BR>#fuses=20
                        HS,NOWDT,NOLVP,NOPUT,BROWNOUT <BR><BR>#define =
XTAL_SPEED=20
                        11000000 <BR><BR>#define MY_BOOTLOADER =
<BR>#include=20
                        &lt;my_bootloader.h&gt;=20
                        =
<BR><BR><BR>/////////////////////////////////////////////////////////////=
///////////////////=20
                        <BR>// Relocate the interrupt vectors. <BR>// =
Depending=20
                        on whether the bootloader or application is =
active=20
                        <BR>// different interrupt dispatchers are =
called.=20
                        <BR>#org HIGH_INT_VECTOR, INTERRUPT_REMAP_END =
<BR>void=20
                        isr_relocate(void) { <BR>#asm <BR>&nbsp; // =
Address=20
                        0x0008 is the hardware High priority interrupt=20
                        <BR>&nbsp; TSTFSZ BootloaderActive&nbsp; &nbsp; =
&nbsp;=20
                        &nbsp;// if bootloader active <BR>&nbsp; =
GOTO&nbsp;=20
                        LOADER_HIGH_INT&nbsp; &nbsp; &nbsp; &nbsp; =
&nbsp;// then=20
                        jump to bootloader ISR <BR>&nbsp; GOTO&nbsp;=20
                        APPLICATION_HIGH_INT&nbsp; &nbsp; // else jump =
to=20
                        application ISR <BR><BR>&nbsp; NOP&nbsp; &nbsp; =
&nbsp;=20
                        &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; =
&nbsp;=20
                        &nbsp; &nbsp; &nbsp;// Just filling memory =
<BR>&nbsp;=20
                        NOP <BR>&nbsp; NOP <BR><BR>&nbsp; // Address =
0x0018 is=20
                        the hardware Low priority interrupt <BR>&nbsp; =
TSTFSZ=20
                        BootloaderActive&nbsp; &nbsp; &nbsp; &nbsp;// if =

                        bootloader active <BR>&nbsp; GOTO&nbsp;=20
                        &nbsp;LOADER_NORMAL_INT&nbsp; &nbsp; &nbsp; // =
then jump=20
                        to bootloader ISR <BR>&nbsp; GOTO&nbsp;=20
                        &nbsp;APPLICATION_NORMAL_INT // else jump to =
application=20
                        ISR <BR>#endasm <BR>} <BR><BR><BR>// Start of =
the=20
                        bootloader program <BR>//#org LOADER_START, =
LOADER_END=20
                        default&nbsp; &nbsp; &nbsp; // Fix: Segment =
setting=20
                        removed, doesn't compile in v3.245 <BR>#use =
delay( clock=20
                        =3D XTAL_SPEED ) // required for delay_ms() =
<BR><BR>//=20
                        Just a simple interrupt handler example =
<BR>#int_timer0=20
                        <BR>void timer0_isr() <BR>{ <BR>&nbsp; int =
i=3D0;=20
                        <BR>&nbsp; i =3D i+1; <BR>} <BR><BR>void =
main(void) <BR>{=20
                        <BR>&nbsp; int8 i; <BR><BR>&nbsp; =
BootloaderActive =3D=20
                        TRUE; <BR>&nbsp; <BR>&nbsp; // Do your =
bootloader things=20
                        here <BR>&nbsp; for (i=3D0; i&lt;10; i++) =
<BR>&nbsp; {=20
                        <BR>&nbsp; &nbsp; output_toggle(PIN_B1); =
<BR>&nbsp;=20
                        &nbsp; delay_ms(500); <BR>&nbsp; } <BR>&nbsp; =
<BR>&nbsp;=20
                        // Exit the bootloader and go to the Real =
Application=20
                        <BR>&nbsp; goto_address(APPLICATION_START);=20
                    <BR>}</TD></TR></TBODY></TABLE><SPAN=20
                  class=3Dpostbody><BR><BR>Real Application</SPAN>
